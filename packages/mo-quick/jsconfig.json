{
  "include": ["src/**/*", "qq/**/*", "mo/**/*", "mo-ui/**/*"],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es2019",
    "checkJs": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "qq/*": ["qq/*"],
      "mo/*": ["mo/*"],
      "mo-ui/*": ["mo-ui/*"],
    }
  }
}
